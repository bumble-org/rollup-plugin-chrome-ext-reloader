    "events.once": "^2.0.2",
    "jest": "^24.2.0-alpha.0",
    "replace-in-file": "^3.4.4",
    "rollup": "^1.6.0",
    "rollup-plugin-auto-manifest": "latest",
    "rollup-plugin-manifest-entry": "latest",
    "simple-git": "^1.107.0",
    "sucrase": "^3.10.0"
